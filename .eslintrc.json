{
    "extends": "standard",
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-unused-vars": "warn",
      "no-useless-escape": 0
    },
    "plugins": [
      "security"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }